@page
@model per.Pages.ConsultoriaModel
@{
    ViewData["Title"] = "Consultoria";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Consultoría</title>
    <link href="~/css/creative.css" rel="stylesheet">
    <link href="~/css/Servicios.css" rel="stylesheet">
</head>
<body>
    <header class="consultoria">
        <div class="container h-100">
            <div class="row h-100 align-items-center justify-content-center text-center">
                <div class="col-lg-10 align-self-end">
                    <div class="video-responsive">
                        <iframe frameborder="0" scrolling="no" type="text/html" src="https://www.youtube.com/embed/FINUvnVHy-w?autoplay=0&fs=0&iv_load_policy=3&showinfo=0&rel=0&cc_load_policy=0&start=0&end=0&origin=https://youtubeembedcode.com"></iframe>
                    </div>
                    <h2 class="text-white mt-0">
                        Consultoría <br />
                    </h2>
                    <hr class="divider light my-4">
                    <button class="btnServicios" onclick="location.href='/Contacto'">Solicita tu servicio!</button>
